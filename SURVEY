<script type="text/javascript">
	function output(gender1, gender2, age1, age2, age3, age4, age5, job1, job2,
			job3, job4, job5, job6, agency, cost1, cost2, cost3, cost4, year,
			month, area) {
		var outgender1 = document.getElementById(gender1).checked;
		var outgender2 = document.getElementById(gender2).checked;
		var outage1 = document.getElementById(age1).checked;
		var outage2 = document.getElementById(age2).checked;
		var outage3 = document.getElementById(age3).checked;
		var outage4 = document.getElementById(age4).checked;
		var outage5 = document.getElementById(age5).checked;
		var outjob1 = document.getElementById(job1).checked;
		var outjob2 = document.getElementById(job2).checked;
		var outjob3 = document.getElementById(job3).checked;
		var outjob4 = document.getElementById(job4).checked;
		var outjob5 = document.getElementById(job5).checked;
		var outjob6 = document.getElementById(job6).checked;
		var outagency = document.getElementById(agency).value;
		var outcost1 = document.getElementById(cost1).checked;
		var outcost2 = document.getElementById(cost2).checked;
		var outcost3 = document.getElementById(cost3).checked;
		var outcost4 = document.getElementById(cost4).checked;
		var outyear = document.getElementById(year).value;
		var outmonth = document.getElementById(month).value;
		var outtext = document.getElementById(area).value;

		if (outgender1 == true) {
			outgender1 = document.getElementById(gender1).value
		} else if (outgender1 == false) {
			outgender1 = ""
		}
		if (outgender2 == true) {
			outgender2 = document.getElementById(gender2).value
		} else if (outgender2 == false) {
			outgender2 = ""
		}

		if (outage1 == true) {
			outage1 = document.getElementById(age1).value
		} else if (outage1 == false) {
			outage1 = ""
		}
		if (outage2 == true) {
			outage2 = document.getElementById(age2).value
		} else if (outage2 == false) {
			outage2 = ""
		}
		if (outage3 == true) {
			outage3 = document.getElementById(age3).value
		} else if (outage3 == false) {
			outage3 = ""
		}
		if (outage4 == true) {
			outage4 = document.getElementById(age4).value
		} else if (outage4 == false) {
			outage4 = ""
		}
		if (outage5 == true) {
			outage5 = document.getElementById(age5).value
		} else if (outage5 == false) {
			outage5 = ""
		}

		if (outjob1 == true) {
			outjob1 = document.getElementById(job1).value
		} else if (outjob1 == false) {
			outjob1 = ""
		}
		if (outjob2 == true) {
			outjob2 = document.getElementById(job2).value
		} else if (outjob2 == false) {
			outjob2 = ""
		}
		if (outjob3 == true) {
			outjob3 = document.getElementById(job3).value
		} else if (outjob3 == false) {
			outjob3 = ""
		}
		if (outjob4 == true) {
			outjob4 = document.getElementById(job4).value
		} else if (outjob4 == false) {
			outjob4 = ""
		}
		if (outjob5 == true) {
			outjob5 = document.getElementById(job5).value
		} else if (outjob5 == false) {
			outjob5 = ""
		}
		if (outjob6 == true) {
			outjob6 = document.getElementById(job6).value
		} else if (outjob6 == false) {
			outjob6 = ""
		}

		if (outcost1 == true) {
			outcost1 = document.getElementById(cost1).value + " "
		} else if (outcost1 == false) {
			outcost1 = ""
		}
		if (outcost2 == true) {
			outcost2 = document.getElementById(cost2).value + " "
		} else if (outcost2 == false) {
			outcost2 = ""
		}
		if (outcost3 == true) {
			outcost3 = document.getElementById(cost3).value + " "
		} else if (outcost3 == false) {
			outcost3 = ""
		}
		if (outcost4 == true) {
			outcost4 = document.getElementById(cost4).value + " "
		} else if (outcost4 == false) {
			outcost4 = ""
		}

		alert("성별 : " + outgender1 + outgender2 + "\n연령대 : " + outage1
				+ outage2 + outage3 + outage4 + outage5 + "\n직업 : " + outjob1
				+ outjob2 + outjob3 + outjob4 + outjob5 + outjob6 + "\n통신사 : "
				+ outagency + "\n요금제 : " + outcost1 + outcost2 + outcost3
				+ outcost4 + "\n사용기간 : " + outyear + " " + outmonth
				+ "\n불편사항 : " + outtext);
	}
</script>

